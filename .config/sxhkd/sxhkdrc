super + Return
	st
super + shift + Return
	scratchpad

super + q
	qutebrowser
super + e
	st -c neomutt neomutt
super + f
	st -e lf.sh
super + a
	hot.sh
super + d
	dmenu_run_res 
super + shift + d
	dmenu_run
super + r
	scratchpad pmath.sh
super + s
	spotify

super + c
	clipmenu
super + shift + c
	clipdel
super + F9
	dmenumount
super + shift + F9
	dmenuumount

super + {F5,F6}
	light {-U,-A} 10; updatebar
super + shift + y
	exitprompt

super + F1
	pamixer -t ; notify-send -t "500" "♫-1%" ; updatebar
super + F2 
	pamixer -d 5 ; notify-send -t "500" "♫$(pamixer --get-volume)% (-5)" ; updatebar
super + F3
	pamixer -i 5 ; notify-send -t "500" "♫$(pamixer --get-volume)% (+5)" ; updatebar
super + shift + F2 
	pamixer -d 25 ; notify-send -t "500" "♫$(pamixer --get-volume)% (-25)" ; updatebar
super + shift + F3
	pamixer -i 25 ; notify-send -t "500" "♫$(pamixer --get-volume)% (+25)" ; updatebar

super + F7
	dmenurecord

super + F8
	st -e nmtui

super + Print
	screenshot
super + Home
	killall dunst &>/dev/null; killall spt &>/dev/null; dunst &; spt
super + End
	killall -SIGUSR1 spt
super + Insert
	killall -SIGUSR2 spt
